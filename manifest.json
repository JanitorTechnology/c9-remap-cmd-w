{
  "manifest_version": 2,
  "name": "Disable ^W",
  "version": "0.1",
  "icons": {
    "16": "icon.svg",
    "32": "icon.svg",
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "description": "A WebExtension to remap ^W in Cloud9 IDE (should delete a word, not close the tab)",
  "commands": {
    "no-ctrl-w": {
      "suggested_key": {
        "default": "Ctrl+W",
        "mac": "Command+W"
      },
      "description": "Overrides the Ctrl+W shortcut"
    },
    "no-cmd-w": {
      "suggested_key": {
        "mac": "Command+W"
      },
      "description": "Overrides the Command-W shortcut on macOS"
    }
  }
}
